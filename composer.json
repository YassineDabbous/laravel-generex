{
    "version": "0.0.1",
    "name": "yaseen/packgen",
    "description": "Laravel package generator with CRUD, Validation, Policies, Events, Notifications, API & views",
    "type": "library",
    "authors": [
        {
            "name": "Yassine Dabbous",
            "email": "yassine.dabbous@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": ["laravel", "crud", "package crud", "laravel gate policies", "laravel generator", "package generator", "laravel package"],
    "require": {
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Yaseen\\PackGen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yaseen\\PackGen\\PackGenServiceProvider"
            ]
        }
    }
}
